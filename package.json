{
  "name": "kdu-toasted",
  "description": "Responsive Touch Compatible Toast plugin for KduJS 2+",
  "version": "1.1.25-2",
  "author": "NKDuy",
  "repository": {
    "type": "git",
    "url": "https://github.com/khanhduy1407/kdu-toasted.git"
  },
  "main": "./dist/kdu-toasted.min.js",
  "typings": "./types/index.d.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.release.js --progress --hide-modules",
    "build-local-watch": "cross-env NODE_ENV=production webpack --config ./build/webpack.release.js --progress --hide-modules --watch",
    "release": "npm run build && npm run core && npm run css",
    "css": "cross-env NODE_ENV=production webpack --config ./build/webpack.release.css.js --progress --hide-modules ",
    "core": "cross-env NODE_ENV=production webpack --config ./build/webpack.release.core.js --progress --hide-modules",
    "es": "babel src --presets babel-preset-es2015 --out-dir dist"
  },
  "keywords": [
    "toast",
    "kdu",
    "kdu-toast",
    "kdu-toasted",
    "toasted",
    "responsive",
    "touch-compatible",
    "touch"
  ],
  "dependencies": {},
  "devDependencies": {
    "animejs": "^2.2.0",
    "autoprefixer": "^7.2.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "cross-env": "^3.2.4",
    "css-loader": "^0.25.0",
    "es6-object-assign": "^1.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.9.0",
    "hammerjs": "^2.0.8",
    "jasmine": "^2.99.0",
    "jasmine-core": "^2.99.1",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-requirejs": "^1.1.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.9.3",
    "postcss-loader": "^2.1.6",
    "requirejs": "^2.3.6",
    "sass-loader": "^6.0.7",
    "shortid": "^2.2.13",
    "style-loader": "^0.18.2",
    "kdu": "^2.5.24",
    "kdu-loader": "^14.0.0",
    "kdu-style-loader": "^4.1.3",
    "kdu-template-compiler": "^2.5.24",
    "webpack": "^2.7.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-dev-server": "^2.11.3"
  }
}
